public class calculateGrades {
    double mark1, mark2;

    public void assignMarks(double mark1, double mark2){
        this.mark1 = mark1;
        this.mark2=mark2;
    }
    double average(){
        return (mark1 + mark2)/2 ;
    }
}
class CircleOpereations{
    public static void main (String args[]){
        calculateGrades c = new calculateGrades();
        c.assignMarks(94,95);
        System.out.println("Average: " +c.average());
    }
}
