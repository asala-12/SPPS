public class calculateGrades {
    double mark1, mark2, mark3, mark4;

    public void assignMarks(double mark1, double mark2, double mark3, double mark4){
        this.mark1 = mark1;
        this.mark2 = mark2;
        this.mark3 = mark3;
        this.mark4 = mark4; 
    }
    double average(){
        return (mark1 + mark2 + mark3 + mark4)/4 ;
    }
}
class CircleOpereations{
    public static void main (String args[]){
        calculateGrades c = new calculateGrades();
        c.assignMarks(94,95,90,80);
        System.out.println("Average: " +c.average());
    }
}
