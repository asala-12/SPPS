public class calculateGrades {
    double mark1, mark2, mark3;

    public void assignMarks(double mark1, double mark2, double mark3){
        this.mark1 = mark1;
        this.mark2 = mark2;
        this.mark3 = mark3;
    }
    double average(){
        return (mark1 + mark2 + mark3)/3 ;
    }
}
class CircleOpereations{
    public static void main (String args[]){
        calculateGrades c = new calculateGrades();
        c.assignMarks(94,95,90);
        System.out.println("Average: " +c.average());
    }
}
